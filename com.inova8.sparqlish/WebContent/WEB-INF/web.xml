	<!-- ============================================================== -->
	<!-- UI5 proxy servlet -->
	<!-- ============================================================== -->

	<servlet>
	<servlet-name>SimpleProxyServlet</servlet-name>
	<servlet-class>com.sap.ui5.proxy.SimpleProxyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>SimpleProxyServlet</servlet-name>
	<url-pattern>/proxy/*</url-pattern>
	</servlet-mapping>


